@page
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@using Microsoft.AspNetCore.Mvc.Localization
@using ODY.Cihazkapinda.Localization
@model ODY.Cihazkapinda.Web.Pages.Admin.Licenses.EditModalModel
@inject IHtmlLocalizer<CihazkapindaResource> L
@{
    Layout = null;
}
<form asp-page="/Admin/Licenses/EditModal">
    <abp-modal size="Default">
        <abp-modal-header title="@L["Licenses"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-input asp-for="licenseEditModal.Id" />
            <abp-input asp-for="licenseEditModal.TenantId" />
            <abp-select asp-for="licenseEditModal.LICENSE_OWNER" asp-items="Model.tenantList" />
            <abp-input asp-for="licenseEditModal.LICENSE" />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</form>